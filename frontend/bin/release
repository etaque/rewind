#!/usr/bin/env bash
set -e
cd $(dirname "$0")/..

BUCKET="rewind.skiffr.me"

export NODE_ENV=production
export AWS_PROFILE=rewind-frontend-uploader

rollup -c rollup.config.js

aws s3 sync --cache-control 'max-age=604800' --exclude index.html dist/ "s3://$BUCKET/"
aws s3 sync --cache-control 'no-cache' dist/ "s3://$BUCKET/"
